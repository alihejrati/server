        <style>
                .fonticon-container>.fonticon-wrap {
                        float: right;
                        width: 10px;
                        height: 0px;
                        line-height: 4.8rem;
                        text-align: center;
                        border: 0px solid #967ADC;
                        border-radius: 0.1875rem;
                        margin-left: 1rem;
                        margin-bottom: 1.5rem;
                        position: relative;
                }

                .fonticon-container>.fonticon-wrap>i {
                        position: absolute;
                        font-size: 1.28rem;
                }

                .fonticon-container>.fonticon-wrap>i:hover {
                        position: absolute;
                        font-size: 1.28rem;
                        color: #967ADC;
                }

                .fonticon-container>.fonticon-wrap:hover {
                        background: transparent;
                        color: transparent;
                }

                .fonticon-container>.fonticon-wrap.youtube {
                        width: 100px;
                }

                table#table.dataTable tbody tr:hover {
                        background-color: #ffa;
                }

                table#table.dataTable tbody tr:hover>.sorting_1 {
                        background-color: #ffa;
                }

                .help {
                        cursor: help;
                }

                .COT {
                        font-size: 10px;
                        break-after: auto;
                        color: teal
                }

                body {
                        margin: 40px;
                }


                .btn-circle {
                        width: 30px;
                        height: 30px;
                        text-align: center;
                        padding: 6px 0;
                        font-size: 12px;
                        line-height: 1.428571429;
                        border-radius: 15px;
                }

                .btn-circle.btn-lg {
                        width: 50px;
                        height: 50px;
                        padding: 10px 16px;
                        font-size: 18px;
                        line-height: 1.33;
                        border-radius: 25px;
                }

                .btn-circle.btn-xl {
                        width: 70px;
                        height: 70px;
                        padding: 10px 16px;
                        font-size: 24px;
                        line-height: 1.33;
                        border-radius: 35px;
                }

                td {
                        max-width: 100px;
                        overflow-x: hidden
                }
        </style>
        <script src="<%-template%>/robust-assets/js/core/libraries/jquery.min.js" type="text/javascript"></script>
