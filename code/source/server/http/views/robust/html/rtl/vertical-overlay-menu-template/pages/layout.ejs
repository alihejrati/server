                .fonticon-container>.fonticon-wrap {
                        float: right;
                        width: 10px;
                        height: 0px;
                        line-height: 4.8rem;
                        text-align: center;
                        border: 0px solid #967ADC;
                        border-radius: 0.1875rem;
                        margin-left: 1rem;
                        margin-bottom: 1.5rem;
                        position: relative;
                }

                .fonticon-container>.fonticon-wrap>i {
